#include <iostream>

using namespace std;

int sir_numere ()
{
    
int suma_numerePare;

suma_numerePare = 0;
cout << "Numerele mai mici decat 10 sunt ";

for (int i = 0; i <= 100 ; i++)
{
  if   (i < 10) 
      {
          if (i % 2 == 0)
         {
        
         suma_numerePare=suma_numerePare+i;
         cout << " " << i;
         continue;
         
         }
            else
            {
                    cout << " " << i;
            }
      }
      
       if ( ( i > 10 && i < 20 ) )
         {
                if (i % 2 == 0)
              {
                 suma_numerePare=suma_numerePare+i;
                 continue;
              }
              
               else 
                   {
                      continue;
                   }
         }      
}
     cout << endl;
     cout <<"Suma numerelor pare este " << suma_numerePare << endl;
     return 0;
}

int multiplii ()
{
    for (int i=3; i <= 100; i++)
   {
    
 if ( (i % 3 == 0) && (i % 5 == 0) )
         
         {
                    cout <<"FizzBuzz"<< endl;
                   //cout << " " << i;
         }   
              else if (i % 3 == 0)
                 
                 {
                       cout <<"Fizz" << endl;
                  //   cout << " " << i;
                 }
                    
                    else if (i % 5 == 0)
                       {
                           cout <<"Buzz" << endl;
                          //  cout << " " << i;
                       }
      
        
  }
return 0;
    }

int main()
{
  sir_numere();
  cout << endl;
  multiplii();
}
